generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Switch {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  switchId  String?  @unique
  isOn      Boolean  @default(false)
  updatedAt DateTime @updatedAt
}
